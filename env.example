# Ethereum Data Pipeline Demo Environment Variables
# Copy this file to .env and fill in your actual values

# Etherscan API Key (required)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Blockchain Configuration
# For Sepolia, set RPC_URL to an HTTPS endpoint and CHAIN_ID=11155111
RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID
CHAIN_ID=11155111
PRIVATE_KEY=your_test_private_key
NFT_ADDR=your_deployed_nft_contract_address
RECIPIENT=recipient_address

# IPFS Configuration (already configured)
IPFS_API_URL=http://127.0.0.1:5002
IPFS_GATEWAY_URL=http://127.0.0.1:8081
